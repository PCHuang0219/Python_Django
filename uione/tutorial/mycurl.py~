import pycurl
impost json
from io import BytesIO

buffer = BytesIO()
c = pycurl.Curl()
#c.setopt(c.URL, 'http://www.google.com')
c.setopt(c.URL, 'https://10.3.0.7:8091/redfish/v1')
c.setopt(c.WRITEDATA, buffer)
c.setopt(c.SSL_VERIFYPEER, False)
c. perform()
# c.close()

body = buffer.getvalue().decode('iso-8859-1')


